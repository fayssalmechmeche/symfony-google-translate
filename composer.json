{
  "name": "zerox-k/symfony-google-translate-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Translator using google",
  "keywords": [],
  "homepage": "https://github.com/zerox-k/symfony-google-translate",
  "license": "MIT",
  "authors": [
    {
      "name": "sabri Hamda",
      "email": "sabri@hamda.ch"
    },
    {
      "name": "Jonathan Kashongwe",
      "email": "jokashongwe@gmail.com"
    },
    {
      "name": "ZeRoX",
      "email": "as234zx12@yandex.ru"
    }
  ],
  "require": {
    "php": "^8.2.0",
    "google/cloud-translate": "1.17.*",
    "symfony/http-kernel": "^7.1",
    "symfony/translation": "7.1.*",
    "symfony/finder": "^7.1",
    "twig/twig": "^3.0",
    "symfony/twig-bundle": "^7.1",
    "symfony/console": "^7.1",
    "ext-curl": "*"
  },
  "require-dev": {
    "symfony/dependency-injection": "~6.4|~7.0",
    "phpunit/phpunit": "^11.2"
  },
  "conflict": {
    "symfony/dependency-injection": "<6.4",
    "symfony/event-dispatcher": "<6.4",
    "symfony/var-dumper": "<6.4"
  },
  "autoload": {
    "psr-4": {
      "ShortHint\\GoogleTranslatorBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
